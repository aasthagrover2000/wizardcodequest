{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "get",
        "post"
      ],
      "route": "/server.py"  // Optional: Specify the route for your function
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
